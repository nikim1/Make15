<Window x:Class="Project_Make15.PVEWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Project_Make15"
        xmlns:controls="clr-namespace:UsrCtrl_GameWindow;assembly=UsrCtrl_GameWindow"
        mc:Ignorable="d"
        Title="PVE Game" MinHeight="500" MinWidth="800"
        MaxHeight="600" MaxWidth="1000" >
    <Grid Background="Azure">
        <Viewbox StretchDirection="Both" Stretch="Uniform">
            <controls:UserControlGameWindow Name="PVPUserControl" BtnPickClick="PVPUserControl_BtnPickClick" ChatEnterClick="PVPUserControl_ChatEnterClick"/>
        </Viewbox>
    </Grid>
</Window>
